#the train/test net protocal buffer definition
net: "examples/vgg16/vgg_train_val.prototxt"
test_iter: 100
test_interval: 500
base_lr: 0.001
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.1
display: 20
max_iter: 10000
snapshot: 5000
snapshot_prefix: "/caffe/examples/vgg16"
solver_mode:GPU
